{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "React application starter kit",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "test": "jest --config ./jest.config.json --coverage",
    "build": "webpack --mode production",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "lint:styles": "stylelint 'src/**/*.scss'",
    "lint:fix-styles": "stylelint --fix 'src/**/*.scss'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumitsj/react-starter-kit.git"
  },
  "author": "Sumit Jadhav",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sumitsj/react-starter-kit/issues"
  },
  "homepage": "https://github.com/sumitsj/react-starter-kit#readme",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.1",
    "prettier": "1.18.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-prettier": "^1.1.1",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "normalize-scss": "^7.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run lint:styles",
      "pre-push": "npm test"
    }
  }
}
